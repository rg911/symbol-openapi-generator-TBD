{
    "name": "{{npmName}}",
    "version": "{{npmVersion}}",
    "description": "NodeJS client for {{npmName}}",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/{{gitUserId}}/{{gitRepoId}}.git"
      },
    "main": "dist/api.js",
    "types": "dist/api.d.ts",
    "scripts": {
        "clean": "rm -Rf node_modules/ *.js",
        "build": "tsc",
        "test": "npm run build && node dist/client.js"
    },
    "bugs": {
       "url": "https://github.com/{{gitUserId}}/{{gitRepoId}}"
    },
    "homepage": "https://github.com/{{gitUserId}}/{{gitRepoId}}",
    "license": "Apache-2.0",
    "dependencies": {
        "bluebird": "^3.5.0",
        "request": "^2.81.0",
        "@types/bluebird": "*",
        "@types/request": "*",
        "rewire": "^3.0.2"
    },
    "devDependencies": {
        "typescript": "^2.4.2",
        "@types/node": "8.10.34"
    }{{#npmRepository}},
    "publishConfig": {
        "registry": "{{npmRepository}}"
    }{{/npmRepository}}
}
